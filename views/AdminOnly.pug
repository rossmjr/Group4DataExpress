extends layout

block content
    table(class='detailGrid')
        tr
            th Id
            th Username
            th Password
            th type
            th Email
            th Age
            th Answer 1
            th Answer 2
            th Answer 3
            th
            th
        each item in userList
            tr
                td
                    a(href='/details/' + item.id) #{item.id}
                td #{item.userName}
                td #{item.pass}
                td #{item.type}
                td #{item.email}
                td #{item.age}
                td #{item.answer1}
                td #{item.answer2}
                td #{item.answer3}
                td
                    a(href='/AdminEdit/' + item.id) Edit
                td
                    a(href='/delete/' + item.id) Delete

    p
    #menu
        a(href='/CreateAccount') Add New User